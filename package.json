{
  "name": "unpaywall-data-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "heroku-postbuild": "npm install --only=dev --no-shrinkwrap && npm run build",
    "start": "node server.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "axios": "^0.18.1",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "vue": "^2.6.11",
    "vue-analytics": "^5.22.1",
    "vue-material": "1.0.0-beta-8",
    "vue-meta": "^1.6.0",
    "vue-moment": "^3.2.0",
    "vue-recaptcha": "^1.3.0",
    "vue-router": "^3.4.3",
    "vue-scrollto": "^2.18.2",
    "vuetify": "^2.2.11",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "node-sass": "^4.14.1",
    "sass-loader": "^13.2.0",
    "vue-cli-plugin-vuetify": "^2.5.8",
    "vue-template-compiler": "^2.6.11"
  },
  "babel": {
    "presets": [
      [
        "@vue/app",
        {
          "useBuiltIns": "entry"
        }
      ]
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
